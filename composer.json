{
  "name": "polynds/common-design-patterns",
  "autoload": {
    "psr-4": {
      "CommonDesignPatterns\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "analyse": "phpstan analyse --memory-limit 1000M -c phpstan.neon",
    "cs-fix": "php-cs-fixer fix $1"
  }
}
